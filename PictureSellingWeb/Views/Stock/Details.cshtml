@model PictureSellingWeb.Models.Stock

@{
    ViewData["Title"] = "Details";
}

<h1>Stock Details</h1>

<div class="row">
    <div class="col-md-4">
        <div class="form-group">
            <label class="control-label">Picture</label>
            <input type="text" class="form-control" value="@Model.Picture.Title" disabled />
            <label class="control-label">Stock Count</label>
            <input type="text" name="StokCount" class="form-control" value="@Model.StokCount" disabled />
        </div>
    </div>
</div>

<div>
    <a asp-action="Index"><button class="btn btn-success">Back to List</button></a>
</div>
